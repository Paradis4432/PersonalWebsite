<script>
  $("body").terminal({
    "help": function() {
      return "This is a help message";
    },
    "hello": function() {
      return "Hello World!";
    },
    "echo": function(arg) {
      return arg;
    },
    "clear": function() {
      return this.clear();
    },
    "exit": function() {
      return this.echo("Bye!");
    }
  }, {
    greetings: "Welcome to the terminal",
    name: "js_demo",
    height: 200,
    prompt: "js_demo> "
  });
</script>